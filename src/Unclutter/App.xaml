<prism:PrismApp x:Class="Unclutter.App"
                xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                xmlns:prism="clr-namespace:Unclutter.Prism;assembly=Unclutter.Prism">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Unclutter;component/Template.xaml" />
                <ResourceDictionary Source="/Unclutter.Theme;component/Theme.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</prism:PrismApp>
